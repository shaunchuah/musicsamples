[run]
omit =
    *apps.py,
    */migrations/*,
    */tests/*,
    *wsgi.py,
    manage.py,
    *__init__.py,
    *asgi.py,
    config/*,
    *config.py,
    *storage_backends.py,
    *tests.py
    *venv/*
    app/management/commands/generate_fake_data.py

[report]
exclude_lines =
    if settings.DEBUG
